import sys

def get_input(text):
    '''Read input from file'''
    input_file = open(text, 'r')
    return [line.strip() for line in input_file]

def calculate_num_offspring(current_number_of_fish, days_left):
    '''Determine how many fish are going to be made'''


def main():
    s = get_input('input.txt')
    fish = list(map(int,s[0].split(',')))
    print(fish)

    current_number_of_fish = 0
    days_left = 80
    current_number_of_fish +=  calculate_num_offspring(current_number_of_fish, days_left)
    
main()
